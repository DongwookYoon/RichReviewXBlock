doctype html
html
    head
        meta(charset='utf-8')
        meta(http-equiv="cache-control" content="max-age=0")
        meta(http-equiv="cache-control" content="no-cache")
        meta(http-equiv="expires" content="0")
        meta(http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT")
        meta(http-equiv="pragma" content="no-cache")
        meta(http-equiv="Cache-control" content="no-store")
        meta(http-equiv="X-UA-Compatible" content="IE=edge")
        meta(name="viewport" content="width=device-width, height=device-height, initial-scale=1")
        title= title
        link(rel='shortcut icon' href='https://richreview.azureedge.net/favicon.ico' type='image/x-icon')
        link(rel='stylesheet', href='https://richreview.azureedge.net/lib/bootstrap-3.2.0-dist/css/bootstrap.min.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
        link(rel='stylesheet', href='//maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css')
        if cur_page == "Viewer"
            link(rel='stylesheet', href='/stylesheets/page_vr.css')
        script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
        script(src='https://richreview.azureedge.net/lib/bootstrap-3.2.0-dist/js/bootstrap.min.js')
    body
        .navbar.navbar-default.navbar-fixed-top(role="navigation" id="r2_navbar")
        .content_table
            .content_header
            .content_body
                #r2_app_page(align="center")
                    #r2_app_container(align="left")

                    script(type='text/javascript', src="/js/viewer_helper.js")
                    script(type="text/javascript")
                        |loadRichReview('#{r2_ctx}');
